# This builds a OS/2 executable the requires EMX RT-I

CC=gcc
OBJ=.o
RM=del
EXE=.exe
CFLAGS=-Zcrtdll -O3 -I../.. -DOS2
LFLAGS=-s -Zcrtdll -O3 -L../../fidoconf -L../../smapi
LIBS= -lfconfemx -lsmapiemx

.SUFFIXES:
.SUFFIXES: .c .o .exe

.c.o:
	$(CC) $(CFLAGS) -c $<

include makefile.inc

all: commonprogs

default: all

clean:
	-$(RM) *$(OBJ)

distclean: clean commondistclean
